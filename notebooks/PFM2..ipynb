    "            raise ValueError(f\"[ERROR] La hoja '{args.sheet}' no contiene columnas {sorted(REQ_CAT)}\")\n",
    "        hoja, header = str(args.sheet), 0\n",
    "        if args.debug:\n",
    "            print(f\"[DEBUG] Cat치logo le칤do -> hoja='{hoja}', header={header}\")\n",
