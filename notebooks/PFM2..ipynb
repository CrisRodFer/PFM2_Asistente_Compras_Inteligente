    "\n",
    "    print(\"\\n\".join(report_lines))\n",
    "    print(\"[OK] Proceso finalizado correctamente.\")\n",
    "\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    filtrar_outliers(IN_CSV, OUT_CSV, OUT_PARQUET, OUT_REPORT)"
